# PostgreSQL Analytics Database
POSTGRES_ANALYTICS_DATAUSER=datauser
POSTGRES_ANALYTICS_DATAPASSWORD=admin
POSTGRES_ANALYTICS_DB=analytics
ANALYTICS_READONLY_USER=analytics_reader
ANALYTICS_READONLY_PASSWORD=admin

# PostgreSQL Airflow Metastore
POSTGRES_AIRFLOW_USER=airflowuser
POSTGRES_AIRFLOW_PASSWORD=admin
POSTGRES_AIRFLOW_DB=airflow
AIRFLOW_READONLY_USER=airflow_reader
AIRFLOW_READONLY_PASSWORD=admin

# ETL User (for data transformations)
ETL_USER=etl_user
ETL_PASSWORD=admin

# Airflow Security Keys
# Generate with: python -c "from cryptography.fernet import Fernet; print(Fernet.generate_key().decode())"
AIRFLOW_FERNET_KEY=your_fernet_key_here
# Generate with: python -c "import secrets; print(secrets.token_hex(32))"
AIRFLOW_SECRET_KEY=your_secret_key_here

# Airflow UID (usually 50000)
AIRFLOW_UID=50000
